<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>	
	<display-name>admin52</display-name>
	<context-param>
		<param-name>contextConfigLocation</param-name>
		<param-value>
			/WEB-INF/applicationContext.xml 
	  		/WEB-INF/applicationContext-gp.xml
	  		/WEB-INF/applicationContext-wf.xml
	  		/WEB-INF/applicationContext-print.xml
	  		/WEB-INF/applicationContext-admin.xml
		</param-value>
	</context-param>
	<!-- 
	<context-param>
        <param-name>config-file</param-name>
        <param-value>/quartz/quartz.properties</param-value>
	</context-param>
    -->
	<filter>
    	<filter-name>applusSafeFilter</filter-name>
        <filter-class>com.anyi.gp.domain.security.ApplusSafeFilter</filter-class>
        <init-param>
        	<param-name>checkLevel</param-name>
         	<param-value>1</param-value>
        </init-param>
		<init-param>
        	<param-name>defaultCheckUrlList</param-name>
         	<param-value>
         		login.action,dispatcher.action?function=passwd,dispatcher.action?function=title,dispatcher.action?function=createMenu,dispatcher.action?function=mainFrame
			</param-value>
        </init-param>
    </filter>
	  
	<!-- 
	<filter>
		<filter-name>requestWrappingFilter</filter-name>
		<filter-class>
			com.anyi.gp.interceptor.RequestWrappingFilter
		</filter-class>
	</filter>
	-->

    <filter-mapping>
    	<filter-name>applusSafeFilter</filter-name>
        <url-pattern>*.jsp</url-pattern>
    </filter-mapping>
	<filter-mapping>
    	<filter-name>applusSafeFilter</filter-name>
        <url-pattern>*.action</url-pattern>
    </filter-mapping>
  	<!-- 
	<filter-mapping>
		<filter-name>requestWrappingFilter</filter-name>
		<url-pattern>*.jsp</url-pattern>
	</filter-mapping>
    -->		
	<listener>
		<listener-class>
			org.springframework.web.context.ContextLoaderListener
		</listener-class>
	</listener>
	<listener>
		<listener-class>com.anyi.gp.context.ConfigLoaderListener</listener-class>
	</listener>
	<listener>
		<listener-class>
			com.anyi.gp.domain.ApplusSessionListener
		</listener-class>
	</listener>
	<listener>
		<listener-class> com.anyi.gp.context.DogCheckListener</listener-class>
	</listener>
	<!-- quartz scheduler 
	<listener>
		<listener-class>org.quartz.ee.servlet.QuartzInitializerListener</listener-class>	
	</listener>
	-->	
	<servlet>
