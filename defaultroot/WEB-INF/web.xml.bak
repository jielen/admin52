  	</servlet-mapping>
  	<servlet-mapping>
  		<servlet-name>GetServerInfoServlet</servlet-name>
  		<url-pattern>/GetServerInfoServlet</url-pattern>
  	</servlet-mapping>
	
  	<servlet-mapping>
  		<servlet-name>webwork</servlet-name>
  		<url-pattern>*.action</url-pattern>
  	</servlet-mapping>
